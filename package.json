{
  "name": "phaser3-plugin-pathbuilder",
  "version": "1.6.0",
  "description": "Draw and edit Lines, Bezier Curves, Splines during runtime and export them for path tweens and PathFollowers in Phaser 3",
  "main": "src/main.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "dist": "webpack --config webpack.dist.config.js",
    "lint": "eslint --config .eslintrc.json \"src/**/*.js\"",
    "lintfix": "eslint --config .eslintrc.json \"src/**/*.js\" --fix",
    "sloc": "node-sloc './src' --include-extensions \"js\""
  },
  "keywords": [
    "curves",
    "Curve",
    "Editor",
    "editor",
    "build",
    "generate",
    "JSON",
    "export",
    "Phaser 3",
    "Phaser",
    "plugin",
    "pathFollowers",
    "pahts",
    "tweens",
    "animation",
    "motion",
    "drawing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samid737/phaser3-plugin-pathbuilder"
  },
  "author": "samid737",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samid737/phaser3-plugin-pathbuilder/issues"
  },
  "homepage": "https://github.com/samid737/phaser3-plugin-pathbuilder#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "eslint": "^4.13.1",
    "fs-extra": "^5.0.0",
    "node-sloc": "^0.1.10",
    "raw-loader": "^0.5.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "uuid": "^3.1.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0"
  }
}
